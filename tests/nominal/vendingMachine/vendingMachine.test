Enum STATE = {pay,select,soda,beer};


Process Machine{
	s: STATE;
	Initial: s==pay;
	Normative: true;
		[insertCoin] s == pay -> s = select;
		[a] internal s == select -> s = soda;
		[a] internal s == select -> s = beer;
		[getSoda] s == soda -> s = pay;
		[getBeer] s == beer -> s = pay;
}



Main(){
    m1:Machine;
    run m1();
}