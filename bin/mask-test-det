#script for replicating experimental evaluation results
echo "----------------------------Memory Cell test----------------------------"
echo "3 bits"
time java -jar $MASKD/maskD.jar -det ../tests/nominal/memoryCell/memoryCell3.test ../tests/faulty/memoryCell/memoryCell3.test
echo "5 bits"
time java -jar $MASKD/maskD.jar -det ../tests/nominal/memoryCell/memoryCell5.test ../tests/faulty/memoryCell/memoryCell5.test
echo "7 bits"
time java -jar $MASKD/maskD.jar -det ../tests/nominal/memoryCell/memoryCell7.test ../tests/faulty/memoryCell/memoryCell7.test
echo "9 bits"
time java -jar $MASKD/maskD.jar -det ../tests/nominal/memoryCell/memoryCell9.test ../tests/faulty/memoryCell/memoryCell9.test
echo "11 bits"
time java -jar $MASKD/maskD.jar -det ../tests/nominal/memoryCell/memoryCell11.test ../tests/faulty/memoryCell/memoryCell11.test

echo "----------------------------N-Modular Redundancy test----------------------------"
echo "3 modules "
time java -jar $MASKD/maskD.jar -det ../tests/nominal/nmr/nmr3.test ../tests/faulty/nmr/nmr3.test
echo "5 modules"
time java -jar $MASKD/maskD.jar -det ../tests/nominal/nmr/nmr5.test ../tests/faulty/nmr/nmr5.test
echo "7 modules"
time java -jar $MASKD/maskD.jar -det ../tests/nominal/nmr/nmr7.test ../tests/faulty/nmr/nmr7.test
echo "9 modules"
time java -jar $MASKD/maskD.jar -det ../tests/nominal/nmr/nmr9.test ../tests/faulty/nmr/nmr9.test
echo "11 modules"
time java -jar $MASKD/maskD.jar -det ../tests/nominal/nmr/nmr11.test ../tests/faulty/nmr/nmr11.test

echo "----------------------------Philosophers test (odd/even model)----------------------------"
echo "2 phils"
time java -jar $MASKD/maskD.jar -det -nb -l ../tests/nominal/phils/phils2.test ../tests/faulty/phils/phils2.test
echo "3 phils"
time java -jar $MASKD/maskD.jar -det -nb -l ../tests/nominal/phils/phils3.test ../tests/faulty/phils/phils3.test
echo "4 phils"
time java -jar $MASKD/maskD.jar -det -nb -l ../tests/nominal/phils/phils4.test ../tests/faulty/phils/phils4.test
echo "5 phils"
time java -jar $MASKD/maskD.jar -det -nb -l ../tests/nominal/phils/phils5.test ../tests/faulty/phils/phils5.test
echo "6 phils"
time java -jar $MASKD/maskD.jar -det -nb -l ../tests/nominal/phils/phils6.test ../tests/faulty/phils/phils6.test

echo "----------------------------Byzantine Generals test----------------------------"
echo "3 generals "
time java -jar $MASKD/maskD.jar -det ../tests/nominal/byzantine/byz3.test ../tests/faulty/byzantine/byz3.test 
echo "4 generals"
time java -jar $MASKD/maskD.jar -det ../tests/nominal/byzantine/byz4.test ../tests/faulty/byzantine/byz4.test

echo "----------------------------Bounded Retransmission Protocol test----------------------------"
echo "1 chunk(s), 1 retransmission(s)"
time java -jar $MASKD/maskD.jar -det ../tests/nominal/brp/brpN1.test ../tests/faulty/brp/brpN1M1.test
echo "1 chunk(s), 5 retransmission(s)"
time java -jar $MASKD/maskD.jar -det ../tests/nominal/brp/brpN1.test ../tests/faulty/brp/brpN1M5.test
echo "1 chunk(s), 10 retransmission(s)"
time java -jar $MASKD/maskD.jar -det ../tests/nominal/brp/brpN1.test ../tests/faulty/brp/brpN1M10.test
echo "1 chunk(s), 20 retransmission(s)"
time java -jar $MASKD/maskD.jar -det ../tests/nominal/brp/brpN1.test ../tests/faulty/brp/brpN1M20.test
echo "5 chunk(s), 1 retransmission(s)"
time java -jar $MASKD/maskD.jar -det ../tests/nominal/brp/brpN5.test ../tests/faulty/brp/brpN5M1.test
echo "5 chunk(s), 5 retransmission(s)"
time java -jar $MASKD/maskD.jar -det ../tests/nominal/brp/brpN5.test ../tests/faulty/brp/brpN5M5.test
echo "5 chunk(s), 10 retransmission(s)"
time java -jar $MASKD/maskD.jar -det ../tests/nominal/brp/brpN5.test ../tests/faulty/brp/brpN5M10.test
echo "5 chunk(s), 20 retransmission(s)"
time java -jar $MASKD/maskD.jar -det ../tests/nominal/brp/brpN5.test ../tests/faulty/brp/brpN5M20.test
echo "10 chunk(s), 1 retransmission(s)"
time java -jar $MASKD/maskD.jar -det ../tests/nominal/brp/brpN10.test ../tests/faulty/brp/brpN10M1.test
echo "10 chunk(s), 5 retransmission(s)"
time java -jar $MASKD/maskD.jar -det ../tests/nominal/brp/brpN10.test ../tests/faulty/brp/brpN10M5.test
echo "10 chunk(s), 10 retransmission(s)"
time java -jar $MASKD/maskD.jar -det ../tests/nominal/brp/brpN10.test ../tests/faulty/brp/brpN10M10.test
echo "10 chunk(s), 20 retransmission(s)"
time java -jar $MASKD/maskD.jar -det ../tests/nominal/brp/brpN10.test ../tests/faulty/brp/brpN10M20.test
